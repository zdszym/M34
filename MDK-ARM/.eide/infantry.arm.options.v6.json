{
    "version": 3,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "use-microLIB": false,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-balanced",
        "language-c": "c99",
        "language-cpp": "c++11",
        "link-time-optimization": false,
        "one-elf-section-per-function": true,
        "short-enums#wchar": true,
        "warnings": "no-warnings"
    },
    "asm-compiler": {
        "$use": "asm-auto"
    },
    "linker": {
        "output-format": "elf",
        "misc-controls": "--diag_suppress=L6329",
        "xo-base": "",
        "ro-base": "",
        "rw-base": ""
    }
}